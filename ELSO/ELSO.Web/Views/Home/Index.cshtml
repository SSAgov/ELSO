            <div ng-app="ElsoApp" class="row">
              <div ng-controller="ElsoController">
    <table class="table">
        <thead>
            <tr>
        <td>Event Name</td>
        <td>Location</td>
        <td>EventStartDate</td>
        <td>EventEndDate</td>
        <td>OrganizerId</td>
        <td>Category</td>
        <td>CreatedDate</td>
        <td>ModifiedDate</td></tr>
        </thead>
        <tr ng-repeat="r in meetings">
            <td>{{r.EventName}}</td>
             <td>{{r.Location}}</td>
             <td>{{r.EventStartDate}}</td>
            <td>{{r.EventEndDate}}</td>
            <td>{{r.OrganizerId}}</td>
            <td>{{r.Category}}</td>
            <td>{{r.CreatedDate}}</td>
            <td>{{r.ModifiedDate}}</td>
         </tr>
       </table>

</div>
            </div>
        </div>
        <div role="tabpanel" class="tab-pane" id="viewPast">...</div>
        <div role="tabpanel" class="tab-pane" id="upcoming">...</div>
        <div role="tabpanel" class="tab-pane" id="contactUs">...</div>
